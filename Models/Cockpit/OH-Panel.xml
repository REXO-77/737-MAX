            <binding>
                <command>property-toggle</command>
                <property>controls/electric/engine[0]/bus-tie</property>
                <property>controls/electric/engine[1]/bus-tie</property>
            </binding>
        </action>
    </animation>

    <!-- Bus Transition Status -->
    <animation>
        <type>select</type>
        <object-name>elec.transoffL</object-name>
        <condition>
            <equals>
                <property>controls/electric/engine[0]/bus-tie</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>elec.transoffR</object-name>
        <condition>
            <equals>
                <property>controls/electric/engine[1]/bus-tie</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>elec.sourceoffL</object-name>
        <condition>
            <equals>
                <property>controls/electric/engine[0]/bus-tie</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>elec.sourceoffR</object-name>
        <condition>
            <equals>
                <property>controls/electric/engine[1]/bus-tie</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>
    
    <!-- Windshield Wipers -->
    <animation>
        <type>knob</type>
        <object-name>wiper.L</object-name>
        <property>controls/OH/wiper-left</property>
        <factor>-45</factor>
        <center>
            <x-m>-0.009907</x-m>
            <y-m>-0.072772</y-m>
            <z-m>-0.403988</z-m>
        </center>
        <axis>
            <x>1</x>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/OH/wiper-left</property>
                <step>1</step>
                <min>0</min>
                <max>4</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>wiper.R</object-name>
        <property>controls/OH/wiper-right</property>
        <factor>-45</factor>
        <center>
            <x-m>-0.009907</x-m>
            <y-m>0.005628</y-m>
            <z-m>-0.403988</z-m>
        </center>
        <axis>
            <x>1</x>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/OH/wiper-right</property>
                <step>1</step>
                <min>0</min>
                <max>4</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>
</PropertyList>