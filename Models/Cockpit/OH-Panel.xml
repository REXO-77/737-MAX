            <z-m>-0.457091</z-m>
        </center>
        <axis>
            <y>1</y>
        </axis>
        <action>
            <binding>
                <command>property-toggle</command>
                <property>controls/lighting/landing-lights</property>
            </binding>
        </action>
    </animation>

    <!-- Runway turnoff -->
