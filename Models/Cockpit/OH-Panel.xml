        </action>
    </animation>

    <!-- Left Tank Pressure -->
    <animation>
        <type>select</type>
        <object-name>fuel.ALpress</object-name>
        <condition>
            <or>
                <equals>
                    <property>controls/fuel/tank[0]/pump-aft</property>
                    <value>0</value>
                </equals>
                <less-than>
                    <property>fdm/jsbsim/propulsion/tank[0]/contents-lbs</property>
                    <value>1</value>
                </less-than>
            </or>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>fuel.FLpress</object-name>
        <condition>
            <or>
                <equals>
                    <property>controls/fuel/tank[0]/pump-fwd</property>
                    <value>0</value>
                </equals>
                <less-than>
                    <property>fdm/jsbsim/propulsion/tank[0]/contents-lbs</property>
                    <value>1</value>
                </less-than>
            </or>
        </condition>
    </animation>

    <!-- Right Tank Pressure -->
    <animation>
        <type>select</type>
        <object-name>fuel.FRpress</object-name>
        <condition>
