<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <path>fo-R.ac</path>

    <animation>
        <type>select</type>
        <object-name>screen4.L</object-name>
        <object-name>screen4.R</object-name>
        <condition>
            <equals>
                <property>sim/fdm-initialized</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>material</type>
        <object-name>screen4.full</object-name>
        <object-name>screen4.L</object-name>
        <object-name>screen4.R</object-name>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
        </emission>
    </animation>
    
    <animation>
        <type>pick</type>
        <object-name>screen4.full</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>boeing737.showPFDF();</script>
            </binding>
        </action>
    </animation>
</PropertyList>