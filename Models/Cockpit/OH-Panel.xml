    <!-- Crossfeed -->
    <animation>
        <type>knob</type>
        <object-name>fuel.crossfeed</object-name>
        <property>controls/fuel/crossfeed</property>
        <factor>90</factor>
        <center>
            <x-m>-0.01514</x-m>
            <y-m>-0.27091</y-m>
            <z-m>-0.27887</z-m>
        </center>
        <axis>
            <x>1</x>
        </axis>
        <action>
            <binding>
                <command>property-toggle</command>
                <property>controls/fuel/crossfeed</property>
            </binding>
        </action>
    </animation>
    <animation>
        <type>select</type>
        <object-name>fuel.crossfeedon</object-name>
        <condition>
            <equals>
                <property>controls/fuel/crossfeed</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>

    <!-- Fuel Temperature -->
    <animation>
        <type>rotate</type>
        <object-name>fuel.temp</object-name>
        <property>consumables/fuel/tank[0]/temperature_degC</property>
        <factor>-1.75</factor>
        <center>
            <x-m>-0.016789</x-m>
            <y-m>-0.271394</y-m>
            <z-m>-0.216875</z-m>
        </center>
        <axis>
            <x>1</x>
        </axis>
    </animation>

