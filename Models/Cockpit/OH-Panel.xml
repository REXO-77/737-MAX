    <!-- Starters -->
    <!-- APU -->
    <animation>
        <type>pick</type>
        <object-name>apu.start</object-name>
        <action>
            <button>0</button>
            <repeatable type="bool">false</repeatable>
            <binding>
                <command>property-cycle</command>
                <property>controls/APU/master</property>
                <value>0</value>
                <value>1</value>
                <value>2</value>
            </binding>
        </action>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>apu.start</object-name>
        <property>controls/APU/master</property>
        <factor>23</factor>
        <center>
            <x-m>-0.027065</x-m>
            <y-m>-0.07862</y-m>
            <z-m>-0.465208</z-m>
        </center>
        <axis>
            <y>1</y>
        </axis>
    </animation>

    <!-- N1 -->
    <animation>
        <type>knob</type>
        <object-name>n1.start</object-name>
        <property>controls/engines/engine[0]/starter</property>
        <factor>-56</factor>
        <center>
            <x-m>-0.004527</x-m>
            <y-m>-0.021022</y-m>
            <z-m>-0.460612</z-m>
